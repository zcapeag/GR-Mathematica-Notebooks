 RowBox[{
  RowBox[{
   RowBox[{"connectionsfunc", "[", 
    RowBox[{"i_", ",", "m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], "*", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"invMetric", "[", 
        RowBox[{"[", 
         RowBox[{"i", ",", "k"}], "]"}], "]"}], "*", 
       RowBox[{"(", 
