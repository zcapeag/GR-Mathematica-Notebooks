           RowBox[{"coordinates", "[", 
            RowBox[{"[", "n", "]"}], "]"}]}], "]"}], "-", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"m", ",", "n"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "k", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "4"}], "}"}]}], "]"}]}]}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"connections", "=", 
