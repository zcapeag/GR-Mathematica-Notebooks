Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"connections", "[", 
    RowBox[{"i_", ",", "m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"invMetric", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "k"}], "]"}], "]"}], "*", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"n", ",", "k"}], "]"}], "]"}], ",", 
          RowBox[{"coordinates", "[", 
           RowBox[{"[", "m", "]"}], "]"}]}], "]"}], "+", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"k", ",", "n"}], "]"}], "]"}], ",", 
          RowBox[{"coordinates", "[", 
           RowBox[{"[", "m", "]"}], "]"}]}], "]"}], "-", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"m", ",", "n"}], "]"}], "]"}], ",", 
          RowBox[{"coordinates", "[", 
           RowBox[{"[", "k", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1", ",", "4"}], "}"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"connections", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"connections", "[", 
       RowBox[{"i", ",", "m", ",", "n"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "4"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"m", ",", "1", ",", "4"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "1", ",", "4"}], "}"}]}], "]"}]}], ";"}], "\n", 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"reimannTensor", "[", 
    RowBox[{"i_", ",", "j_", ",", "k_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"connections", "[", 
         RowBox[{"[", 
          RowBox[{"l", ",", "i", ",", "k"}], "]"}], "]"}], ",", 
        RowBox[{"coordinates", "[", 
         RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "-", 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"connections", "[", 
         RowBox[{"[", 
