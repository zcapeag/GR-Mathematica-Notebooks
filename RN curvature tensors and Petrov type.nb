        RowBox[{"coordinates", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "+", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"connections", "[", 
          RowBox[{"[", 
           RowBox[{"p", ",", "i", ",", "k"}], "]"}], "]"}], "*", 
         RowBox[{"connections", "[", 
          RowBox[{"[", 
           RowBox[{"l", ",", "j", ",", "p"}], "]"}], "]"}]}], "-", 
        RowBox[{
         RowBox[{"connections", "[", 
          RowBox[{"[", 
           RowBox[{"p", ",", "j", ",", "k"}], "]"}], "]"}], "*", 
         RowBox[{"connections", "[", 
          RowBox[{"[", 
           RowBox[{"l", ",", "i", ",", "p"}], "]"}], "]"}]}]}], ")"}]}], ",", 
     
     RowBox[{"{", 
      RowBox[{"p", ",", "1", ",", "4"}], "}"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
