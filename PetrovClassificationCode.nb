          RowBox[{"i", ",", "m"}], "]"}], "]"}]}], "-", 
       RowBox[{
        RowBox[{"ricciTensor", "[", 
         RowBox[{"[", 
          RowBox[{"k", ",", "m"}], "]"}], "]"}], "*", 
        RowBox[{"metric", "[", 
         RowBox[{"[", 
