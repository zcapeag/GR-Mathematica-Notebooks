Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"connectionsfunc", "[", 
    RowBox[{"i_", ",", "m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], "*", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"invMetric", "[", 
        RowBox[{"[", 
         RowBox[{"i", ",", "k"}], "]"}], "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"n", ",", "k"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "m", "]"}], "]"}]}], "]"}], "+", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"k", ",", "m"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "n", "]"}], "]"}]}], "]"}], "-", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"m", ",", "n"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "k", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "4"}], "}"}]}], "]"}]}]}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"connections", "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"connectionsfunc", "[", 
        RowBox[{"i", ",", "m", ",", "n"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "4"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", "4"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "4"}], "}"}]}], "]"}], "]"}]}], ";"}], 
  "\n", "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"reimannTensorfunc", "[", 
    RowBox[{"a_", ",", "d_", ",", "f_", ",", "b_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"connections", "[", 
       RowBox[{"[", 
        RowBox[{"b", ",", "a", ",", "f"}], "]"}], "]"}], ",", 
      RowBox[{"coordinates", "[", 
       RowBox[{"[", "d", "]"}], "]"}]}], "]"}], "-", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"connections", "[", 
       RowBox[{"[", 
        RowBox[{"b", ",", "d", ",", "f"}], "]"}], "]"}], ",", 
      RowBox[{"coordinates", "[", 
       RowBox[{"[", "a", "]"}], "]"}]}], "]"}], "+", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"connections", "[", 
        RowBox[{"[", 
         RowBox[{"b", ",", "d", ",", "c"}], "]"}], "]"}], "*", 
       RowBox[{"connections", "[", 
        RowBox[{"[", 
         RowBox[{"c", ",", "a", ",", "f"}], "]"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"c", ",", "1", ",", "4"}], "}"}]}], "]"}], "-", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"connections", "[", 
        RowBox[{"[", 
         RowBox[{"b", ",", "a", ",", "s"}], "]"}], "]"}], "*", 
       RowBox[{"connections", "[", 
        RowBox[{"[", 
         RowBox[{"s", ",", "d", ",", "f"}], "]"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"s", ",", "1", ",", "4"}], "}"}]}], "]"}]}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"reimannTensor", "=", 
    RowBox[{"Table", "[", 
