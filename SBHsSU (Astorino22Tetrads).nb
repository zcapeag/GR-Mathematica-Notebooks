 RowBox[{
  RowBox[{
   RowBox[{"connectionsfunc", "[", 
    RowBox[{"i_", ",", "m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], "*", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"invMetric", "[", 
        RowBox[{"[", 
         RowBox[{"i", ",", "k"}], "]"}], "]"}], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"n", ",", "k"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "m", "]"}], "]"}]}], "]"}], "+", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"k", ",", "m"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "n", "]"}], "]"}]}], "]"}], "-", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"metric", "[", 
            RowBox[{"[", 
             RowBox[{"m", ",", "n"}], "]"}], "]"}], ",", 
           RowBox[{"coordinates", "[", 
            RowBox[{"[", "k", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "1", ",", "4"}], "}"}]}], "]"}]}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"connections", "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"connectionsfunc", "[", 
        RowBox[{"i", ",", "m", ",", "n"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "4"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", "4"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "4"}], "}"}]}], "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\n"}], "\n", 
 RowBox[{
