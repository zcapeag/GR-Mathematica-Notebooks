    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"metric", "[", 
         RowBox[{"[", 
          RowBox[{"a", ",", "c"}], "]"}], "]"}], "*", 
        RowBox[{"ricciTensor", "[", 
         RowBox[{"[", 
          RowBox[{"d", ",", "b"}], "]"}], "]"}]}], "-", 
